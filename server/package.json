{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently .env.dev \"npm run build:watch\" \"npm run watch:data\"",
    "build:watch": "env-cmd -f .env.dev tsc-watch --onSuccess \"node dist/src/index.js\"",
    "copy:json": "copyfiles -u 1 ./src/data/*.json ./dist/src",
    "watch:data": "if [ ! -f \"./dist/src/data/products.json\" ]; then npm run copy:json; fi && onchange 'src/data/*.json' -- npm run copy:json",
    "build": "env-cmd -f .env.dev tsc && npm run copy:json",
    "deploy": "env-cmd -f .env.dev npm run build && vercel --prod",
    "test": "env-cmd -f .env.dev jest --verbose --coverage ./tests/**/*.spec.ts",
    "prettier": "prettier --write 'src/**/*.{ts,js}'",
    "lint": "eslint '**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "all": "^0.0.0",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "nodemailer": "^6.9.1",
    "pino": "^8.10.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/nodemailer": "^6.4.7",
    "@types/prettier": "^2.7.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/parser": "^5.51.0",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "eslint-plugin-no-unused-expressions": "^0.1.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.20",
    "onchange": "^7.1.0",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "tsc-watch": "^6.0.0"
  }
}
